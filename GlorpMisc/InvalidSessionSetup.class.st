"
InvalidSessionSetup will be raised if the descriptor system we are validating is different from the current session descriptor.


"
Class {
	#name : #InvalidSessionSetup,
	#superclass : #ValidationError,
	#category : #GlorpMisc
}

{ #category : #'VisualWorks metadata' }
InvalidSessionSetup class >> visualWorksMetadata [

	^ #(
		'namespace' 'Glorp'
		'superclassNamespace' 'Glorp'
	)

]
